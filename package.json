{
  "name": "code",
  "description": "hand write code",
  "main": "",
  "scripts": {
    "algorithm:bfs": "cross-env RUN=1 INPUT=./algorithm/bfs.js rollup -c -w",
    "algorithm:dfs": "cross-env RUN=1 INPUT=./algorithm/dfs.js rollup -c -w",
    "datastructure:lru": "cross-env RUN=1 INPUT=./data-structure/lru.js rollup -c -w",
    "datastructure:link-list": "cross-env RUN=1 INPUT=./data-structure/link-list.js rollup -c -w",
    "module:mini-vue": "cross-env RUN=1 INPUT=./module/mini-vue/index.js rollup -c -w",
    "test:mini-vue": "mocha ./module/mini-vue/test/index.js",
    "watch:test:mini-vue": "npm-watch test:mini-vue",
    "module:webpack": "nodemon ./module/webpack/index.js",
    "module:markdown": "cross-env HTML=1 RUN=1 GULP=1 SERVE=1 INPUT=./module/markdown-parser/index.js rollup -c -w",
    "module:html-parser": "cross-env RUN=1 INPUT=./module/html-parser/index.js rollup -c -w",
    "test:html-parser": "mocha ./module/html-parser/test/index.js",
    "watch:test:html-parser": "npm-watch test:html-parser",
    "module:template-parser": "cross-env RUN=1 INPUT=./module/template-parser/index.js rollup -c -w",
    "test:template-parser": "mocha ./module/template-parser/test/index.js",
    "watch:test:template-parser": "npm-watch test:template-parser",
    "module:html-tokenizer": "cross-env RUN=1 INPUT=./module/html-tokenizer/index.js rollup -c -w",
    "module:react": "cross-env HTML=1 RUN=1 GULP=1 SERVE=1 INPUT=./module/react/index.js rollup -c -w",
    "polyfill:promise": "nodemon ./polyfill/promise/promise.js",
    "polyfill:promise-20loc": "nodemon ./polyfill/promise/promise-20loc.js",
    "polyfill:generator": "nodemon ./polyfill/generator.js",
    "polyfill:instanceof": "nodemon ./polyfill/instanceof.js",
    "polyfill:bind": "nodemon ./polyfill/bind.js",
    "polyfill:apply": "nodemon ./polyfill/apply.js",
    "polyfill:call": "nodemon ./polyfill/call.js"
  },
  "watch": {
    "test-html-parser": {
      "see": "https://github.com/M-Zuber/npm-watch/issues/91",
      "patterns": [
        "data-structure/**/*",
        "utils/**/*",
        "build/module/html-parser",
        "module/html-parser"
      ],
      "delay": 1000
    },
    "test-template-parser": {
      "see": "https://github.com/M-Zuber/npm-watch/issues/91",
      "patterns": [
        "build/module/template-parser",
        "module/template-parser"
      ],
      "delay": 1200
    }
  },
  "author": "Lionad",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.10.5",
    "@babel/parser": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/traverse": "^7.10.5"
  },
  "devDependencies": {
    "@rollup/plugin-run": "^2.0.2",
    "cross-env": "^7.0.2",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-load-plugins": "^2.0.3",
    "gulp-rename": "^2.0.0",
    "gulp-stylus": "^2.7.0",
    "mocha": "^10.0.0",
    "node-cmd": "^3.0.0",
    "nodemon": "^2.0.13",
    "npm-watch": "^0.11.0",
    "prettier": "^2.0.5",
    "rollup": "^2.19.0",
    "rollup-plugin-browsersync": "^1.1.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-generate-html-template": "^1.7.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "should": "^13.2.3"
  }
}
